{
   "name": "serverless-nextjs-typescript",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "yarn lint:next && yarn lint:tsc && yarn lint:eslint",
      "lint:next": "next lint",
      "lint:tsc": "tsc --noEmit",
      "lint:eslint": "eslint .",
      "format:prettier": "yarn prettier --write .",
      "prepare": "husky install"
   },
   "dependencies": {
      "@emotion/react": "^11.8.2",
      "@emotion/server": "^11.4.0",
      "@emotion/styled": "^11.8.1",
      "@mui/lab": "^5.0.0-alpha.72",
      "@mui/material": "^5.5.0",
      "clsx": "^1.1.1",
      "next": "12.1.0",
      "react": "17.0.2",
      "react-dom": "17.0.2"
   },
   "devDependencies": {
      "@types/node": "17.0.21",
      "@types/react": "17.0.40",
      "@typescript-eslint/eslint-plugin": "^5.14.0",
      "eslint": "8.11.0",
      "eslint-config-next": "12.1.0",
      "eslint-config-prettier": "^8.5.0",
      "husky": "^7.0.4",
      "lint-staged": "^12.3.5",
      "prettier": "^2.5.1",
      "typescript": "4.6.2"
   },
   "lint-staged": {
      "**/*.{ts,tsx}": [
         "tsc --noEmit",
         "eslint --fix"
      ],
      "**/*.{ts,tsx,json}": [
         "prettier --write"
      ]
   }
}
